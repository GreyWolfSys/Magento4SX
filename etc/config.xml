<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <csp>
            <mode>
                <storefront>
                    <report_uri>https://www.example.com/</report_uri>
                </storefront>
                <admin>
                    <report_uri>https://www.example.com/</report_uri>
                </admin>
            </mode>

        </csp>
        <connectivity>
            <webservices>
                <apikey></apikey>
                <apiurl>https://gwws.greywolf.com/sx/</apiurl>
                <gwcustno></gwcustno>
            </webservices>
            <maxrecall>
                <maxrecall></maxrecall>
                <maxrecalluid></maxrecalluid>
                <maxrecallpwd></maxrecallpwd>
            </maxrecall>
        </connectivity>
        <defaults>
            <gwcustomer>
                <erpcustomerid></erpcustomerid>
                <cono>1</cono>
                <whse></whse>
                <shipviaty></shipviaty>
                <flatshipvia></flatshipvia>
            </gwcustomer>
            <shoppingcart>
                <transtype>qu</transtype>
                <defaultterms></defaultterms>
                <slsrepin>hs</slsrepin>
                <slsrepout>hs</slsrepout>
                <takenby>hs</takenby>
                <taxtakenby>hs</taxtakenby>
                <operinit>sys</operinit>
                <sendtoerpinv>1</sendtoerpinv>
                <autoinvoice>0</autoinvoice>
                <hidepmt></hidepmt>
                <blockpofordefault>0</blockpofordefault>
            </shoppingcart>
            <misc>
                <updateqty>1</updateqty>
                <alertemail></alertemail>
                <debugenabled>0</debugenabled>
                <apiprofiler>0</apiprofiler>
            </misc>
        </defaults>
    </default>
</config>
